    <string name="locale_SYSTEM_value">0</string>
    <string name="locale_EN_value">1</string>
    <string name="locale_DE_value">2</string>
    <string-array name="locale_values">
        <item>@string/locale_SYSTEM_value</item>
        <item>@string/locale_EN_value</item>
        <item>@string/locale_DE_value</item>
    </string-array>

</resources>